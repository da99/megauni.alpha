include "../../App/layout.blade"

- if (customer_has_one_life)
  - var S = '';
- else
  - var S = 's';

append scripts
  call script("/App/top_slash/index.js")

append content

  div#Logo
    span.main
      | ok
    span.sub
      | doki

  h1
    | My Control Panel

  div.box#Screen_Names
    h3 My Life#{S}:
    div.content
      - for (var sn in customer_screen_names)
        div
          | #{customer_screen_names[sn]} -
          a(href="/me/#{customer_screen_names[sn]}") Go to homepage.

